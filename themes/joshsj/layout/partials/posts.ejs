<% posts.forEach(post=> { %>
  <section class="card">
    <h2>
      <a class="mask" href="<%- url_for(post.path) %>">
        <%- post.title %>
      </a>
    </h2>
    
    <p class="extra squish sep sep-pop">
      <span><%- date(post.date) %></span>
      <%- partial("partials/post/tags", { tags: post.tags, mask: true }) %>
    </p>

    <%-  post.excerpt %>

    <p><a href="<%- url_for(post.path) %>">Read</a></p>
  </section>
  <% }) %>