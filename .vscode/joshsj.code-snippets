  "Math Tag Helper": {
    "prefix": ["math"],
    "body": ["{% tex $0 %}"]
  },
  "Big-O Tag Helper": {
    "prefix": ["bigo"],
    "body": ["{% bigo $0 %}"]
  },
  "Captioned Image": {
    "prefix": ["img", "caption_img"],
    "body": "{% caption_img \"${0:src}\" \"${1:caption}\" ${2:source} %}"
  },
  "Inline Header": {
    "prefix": ["ih", "hi", "inline_header"],
    "body": "<span role=\"heading\" aria-level=\"${1:4}\">$0</span>"
  }
